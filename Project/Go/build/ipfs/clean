PROJECT=${PWD##*/}
CONTAINER="golang"

../ENV ${CONTAINER}

echo -e "Now enter container \033[36m${CONTAINER}\033[0m and build \033[36m${PROJECT}\033[0m project"
docker exec -it ${CONTAINER} bash -c "cd /project/DAVE/Project/Go/build && ./CLEAN ${PROJECT}"
