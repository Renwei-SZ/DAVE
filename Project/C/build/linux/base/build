PROJECT=${PWD##*/}
LEVEL=$1
if [ "$LEVEL" == "" ]; then
   LEVEL="alpha"
fi
FORM=$2
if [ "$FORM" == "" ]; then
   FORM="BIN"
fi

chmod a+x ../ENV
../ENV ${PROJECT} ${LEVEL}

cd ../
chmod a+x BUILD
./BUILD ${PROJECT} ${LEVEL} ${FORM}