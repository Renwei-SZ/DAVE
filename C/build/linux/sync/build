PROJECT=${PWD##*/}
PROJECT=${PROJECT^^}

make -C ../ VER_PRODUCT=${PROJECT} LEVEL_PRODUCT=$1 clean_bin
make -C ../ VER_PRODUCT=${PROJECT} LEVEL_PRODUCT=$1 pre_build
make -j8 -C ../ VER_PRODUCT=${PROJECT} LEVEL_PRODUCT=$1
make -C ../ VER_PRODUCT=${PROJECT} LEVEL_PRODUCT=$1 running_bin
mv ../dave ./
mv ../${PROJECT}-BIN ./